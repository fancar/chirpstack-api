    },
    "/api/dataexport/devices/count_frames": {
      "get": {
        "summary": "CountDeviceFramesPerDevEui counts frames for device-list and\\or period between start and end",
        "operationId": "DataExportService_CountDeviceFramesPerDevEui",
        "responses": {
          "200": {
            "description": "A successful response.",
