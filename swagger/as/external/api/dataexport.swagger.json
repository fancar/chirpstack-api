          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "organizationId",
            "description": "id of rhe organnization",
            "in": "query",
            "required": false,
            "type": "integer",
            "format": "int64"
          },
          {
            "name": "devices",
            "description": "comma separated list of dev_eui strings",
            "in": "query",
            "required": false,
            "type": "array",
            "items": {
              "type": "string"
            },
            "collectionFormat": "multi"
          },
          {
            "name": "start",
            "description": "format RFC3339: 2006-01-02T15:04:05.000Z",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "end",
            "description": "format RFC3339: 2006-01-02T15:04:05.000Z",
            "in": "query",
            "required": false,
            "type": "string"
          }
        ],
        "tags": [
          "DataExportService"
        ]
      }
    },
